<?xml version="1.0" encoding="UTF-8"?>
<Installer>
    <Name>ScET</Name>
    <Version>1.0.1</Version>
    <Title>ScET Installer</Title>
    <Publisher>Card Centric</Publisher>
	<ProductUrl>http://en.cardcentric.com/</ProductUrl>
    <StartMenuDir>Card Centric Ltd.</StartMenuDir>
    <TargetDir>@ApplicationsDirX64@/ScET</TargetDir>
	<RemoteRepositories>
        <Repository>
            <Url>https://mourke.github.io/Windows/</Url>
        </Repository>
    </RemoteRepositories>
	<MaintenanceToolName>Uninstaller</MaintenanceToolName>
	<RepositorySettingsPageVisible>false</RepositorySettingsPageVisible>
	<RunProgram>@TargetDir@/ScET.exe</RunProgram>
	<RunProgramDescription>Launch ScET</RunProgramDescription>
</Installer>
